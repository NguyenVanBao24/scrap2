const isLogin = await page?.waitForSelector('.loginPage.oktaLoginPage');
if (isLogin) {
  // Điền vào trường username
  await page.type('#okta-signin-username', 'prospector@ballerheadwear.com');

  // Điền vào trường password
  await page.type('#okta-signin-password', 'Prospector---2024');

  // Click vào nút "Log In"
  await page.click('#okta-signin-submit');

  // Chờ để đảm bảo quá trình đăng nhập hoàn tất (tuỳ thuộc vào trang, bạn có thể cần chờ thêm hoặc điều chỉnh)
  await page.waitForNavigation();
}